					if (win == 4) 
						deck.next(cards);
					
					else if (life == 0) 
					{
						// 목숨(남은 기회)가 0이 될 경우 전 페이지로 돌아가 다시 게임 시작 //
						deck.previous(cards);
						// 목숨, 승리 횟수 초기화 //
						life = 10;
						win = 0;
						// 목숨, 승리 나타내는 텍스트 초기화 //
						label4.setText("life: "+ life);
						label3.setText("win: " + +win);
					}
				}
