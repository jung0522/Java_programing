			 */
			protected void showGameResult( int player, int ball ) 
			{
				cup[ ball ].setIcon( img[ BALL ] ); 
				if ( player != ball )
					cup[ player ].setIcon( img[ MISS ] ); 
				else {
					// 구슬이 담긴 컵을 맞출 경우 win 1 증가 //
					win++;
					// 사용자가 볼 수 있는 승리 텍스트 업데이트 // 
				    label3.setText("win: "+win);
				}
