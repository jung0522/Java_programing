
public class formatting
{
	
	public static void main(String[] args)
	{
		int a = 2;
		int b = 3;
	    System.out.println("안녕"+a*b);//자바에서 문자열+숫자는 문자로 인식
	    System.out.println(2*3);
	    
	    double c = 53234.52450434;
	    System.out.printf("%f\n", c); // 소수점 6자리가지
	    System.out.printf("%10.2f\n", c); // a.b a는 전체 자릿수, b는 소수점 자릿수
	    System.out.printf("%010.2f\n", c); // 빈자리 0 붙이기
	    System.out.printf("%10.2f\n", c);	}
}