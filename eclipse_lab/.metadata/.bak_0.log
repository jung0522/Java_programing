					    	   win++;
					    	   label3.setText("win: " + +win);
					    	   if (win == 5) 
									deck.next(cards);
					        	
					       }
					       // Straight: 숫자가 차례로 이어지는 경우
					       else  if ( ( Numbers[0] + 1 == Numbers[1]  ) &&
						    		( Numbers[1] + 1 == Numbers[2] ) )
