								label3.setText("win: " + +win);
							}
					       // Trips: 자리 숫자가 모두 같은 경우
					       else  if ( ( Numbers[0] == Numbers[1]  ) &&
						    		( Numbers[1] == Numbers[2] ) )
						       {
						    	   result.setText("Trips");
						    	   // 승리 +1
