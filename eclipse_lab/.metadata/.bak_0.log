			{
				
			}
				
			else {
				life--;
				label4.setText("life: "+life);
			}
			imgPlayerA.setIcon( image[ playerA[0] ][ playerA[1] ] );
			imgPlayerB.setIcon( image[ playerB[0] ][ playerB[1] ] );	
			
