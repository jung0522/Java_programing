				if (win == 3) 
				
					deck.next(cards);
					
				
				else if (life == 0) 
				{
					// 목숨(남은 기회) 0이면 전 페이지로 돌아가 다시 게임 진행 //
					deck.previous(cards);
					// 목숨, 승리 초기화 //
					life = 10;
					win = 0;
					// 사용자에게 알려주는 텍스트도 초기화 //
					label4.setText("life: "+ life);
					label3.setText("win: " + +win);
				}	
			
				
			}
		}
