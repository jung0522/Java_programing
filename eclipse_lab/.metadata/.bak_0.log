							   label4.setText("life: "+ life);
					    	  
					    	   result.setText("Miss");
					    	
					       }
					       
					   }
					
					   else
					   {
					   
					   // 숫자 계속해서 변경되게 타이머 스타트
					   timer.start();
					   // 버튼 숫자 멈추게 Stop!으로 업데이트
					   button.setText("Stop!");
					   // 조합 결과 텍스트 초기화
					   result.setText("Combination");
					   }
				      
